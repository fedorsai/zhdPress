<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
  </configSections>
  <appSettings>
    <add key="PrinterName" value="HP402n" />
    <add key="ReportPath" value="Reports/ZDPress.rdlc" />
    <add key="RegisterTemplatePath" value="C:\template\register.xlsx" />
    <add key="PassportTemplatePath" value="C:\template\passport.xlsx" />
    <add key="PassportsArhivePath" value="C:\PassportArchive\" />
    <add key="OpcRequestInterval" value="200" />
    <add key="WithFakeOpc" value="False" />
  </appSettings>
  <connectionStrings>
    <add name="ZDPress" connectionString="Data Source=DESKTOP-D1PA1QJ\SQLEXPRESS;Initial Catalog=ZDPress;Integrated Security=True" providerName="System.Data.SqlClient" />
  </connectionStrings>
  <log4net>
    <appender name="FileAppender" type="log4net.Appender.FileAppender">
      <param name="File" value="C:\Logs\Logs.txt" />
      <param name="AppendToFile" value="true" />
      <layout type="log4net.Layout.PatternLayout">
        <param name="ConversionPattern" value="%d [%t] %-5p %c [%x] - %m%n" />
      </layout>
    </appender>
    <root>
      <level value="DEBUG" />
      <appender-ref ref="FileAppender" />
    </root>
  </log4net>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
</configuration>